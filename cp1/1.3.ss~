#reader(lib "htdp-advanced-reader.ss" "lang")((modname |1.3|) (read-case-sensitive #t) (teachpacks ()) (htdp-settings #(#t constructor repeating-decimal #t #t none #f ())))
(define (big a b c)
  (cond ((and (> a b) (> a c) (> b c)) (+ a b))
        (else (+ b c))
        ))